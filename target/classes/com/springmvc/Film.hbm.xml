<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.springmvc.Film"
		table="film">
		<id name="film_id" column="film_id">
			<generator class="increment" />
		</id>
		<property name="film_title" column="title" />
		<property name="description" column="description" />
		<property name="release_date" column="release_year" />
		<property name="rental_rate" column="rental_rate" />
		
	</class>
</hibernate-mapping>
